# Local Development Environment Configuration
# Copy this file to .env and fill in your values

# Twitch Client ID (not a secret, can be plain text)
TWITCH_ID=your_twitch_client_id_here

# 1Password Service Account Token (optional - only if using 1Password)
# If not using 1Password, leave empty and use raw env vars in prod.env
OP_SERVICE_ACCOUNT_TOKEN=

# Discord Token (if not using 1Password, set here)
# DISCORD_TOKEN=your_discord_token

# Twitch OAuth (if not using 1Password, set here)
# TWITCH_SECRET=your_twitch_client_secret
# TWITCH_TOKEN=your_twitch_oauth_token
